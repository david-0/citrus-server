{
  "name": "citrus-server",
  "version": "0.0.1",
  "description": "Serverpart",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/david-0/citrus-server.git"
  },
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon build/app.js",
    "start:dev": "concurrently \"tsc -w\" \"nodemon build/app.js\"",
    "build": "tsc",
    "start": "ts-node src/app.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "npm run typeorm migration:run -- -d ./src/utils/app-data-source.ts",
    "migration:gen": "npm run typeorm migration:generate -- -d ./src/utils/app-data-source.ts src/migration/schema-migration",
    "clean": "rm -rf dist",
    "serve": "nodemon src/app.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/luxon": "^3.4.2",
    "@types/mime": "^3.0.4",
    "@types/multiparty": "0.0.36",
    "@types/node": "^20.11.10",
    "@types/nodemailer": "^6.4.14",
    "@types/pdfkit": "^0.13.3",
    "@types/pg": "^8.11.0",
    "@types/request": "^2.48.12",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.11.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "citrus-common": "^0.14.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "log4js": "^6.9.1",
    "luxon": "^3.4.4",
    "mime": "^4.0.1",
    "multiparty": "^4.2.3",
    "nodemailer": "^6.9.8",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "npm-check-updates": "^16.14.14"
  }
}
